<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>ActivibeHandler</servlet-name>
		<servlet-class>com.activibe.gae.java.ActivibeRequestHandler</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ActivibeStatusHandler</servlet-name>
		<servlet-class>com.activibe.gae.java.ActivibeStatusRequestHandler</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>ActivibeHandler</servlet-name>
		<url-pattern>/activibe</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ActivibeStatusHandler</servlet-name>
		<url-pattern>/status</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>main.html</welcome-file>
	</welcome-file-list>
</web-app>

		